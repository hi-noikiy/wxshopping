@-webkit-keyframes load{0%{opacity: 1;} 100%{opacity: 0.2;} }
.loadEffect text:nth-child(1){left: 0; top: 50%; margin-top:-2px; -webkit-animation-delay:0.13s;}
.loadEffect text:nth-child(2){left: 3px; top: 3px; -webkit-animation-delay:0.26s;} 
